<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Year</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        {% for injunction in injunctions %}
            <tr>
                <td>
                    <a href="{{ path('injunction_show', { 'id': injunction.id }) }}">
                        {{ injunction.id }}
                    </a>
                </td>
                <td>
                    {{ injunction.title }}
                </td>
                <td>
                    {{ injunction.year }}
                </td>
                <td>
                    {{ injunction.description|raw }}
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
