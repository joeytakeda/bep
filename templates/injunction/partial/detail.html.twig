<table class='table table-bordered table-condensed table-hover table-striped'>
    <tbody>
        <tr>
            <th>Title</th>
            <td>
                {{ injunction.title }}
            </td>
        </tr>
        <tr>
            <th>Year</th>
            <td>{{ injunction.year }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>
                {{ injunction.description|raw }}
            </td>
        </tr>
        <tr>
            <th>Estc</th>
            <td>
                {{ injunction.estc }}
            </td>
        </tr>
        <tr>
            <th>Transactions</th>
            <td>
                {% if injunction.transactions|length > 0 %}
                    <ul>
                        {% for transaction in injunction.transactions %}
                            <li>
                                <a href='{{ path("transaction_show", {"id":transaction.id }) }}'>
                                    {{ transaction }}
                                </a>
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </td>
        </tr>
        <tr>
            <th>Links</th>
            <td>
                {% embed '@NinesMedia/link/partial/list.html.twig' with {
                    'entity': injunction } %}
                {% endembed %}
            </td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ injunction.created|date }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ injunction.updated|date }}</td>
        </tr>
    </tbody>
</table>


